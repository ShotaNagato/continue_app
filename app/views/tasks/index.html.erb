<div class="col-md-offset-4 col-md-4">
 <h1>やること</h1>
</div>

<div class="col-md-offset-4 col-md-4"> 
 <h2>やることリスト</h2>
</div>

<div class="col-md-offset-4 col-md-4"> 
 <% @tasks.each do |task| %>
<div>
  <%= task.title %>
</div>
   <%= task.start_time.strftime("%Y-%m-%d %H:%M") %>
   <%= link_to '変更', edit_task_path(task) %>
   <%= link_to '終了！', task_path(task), method: :delete %>
  
 <% end %>
 <%= link_to '追加', new_task_path %>
</div>


  
  
  
  
  



