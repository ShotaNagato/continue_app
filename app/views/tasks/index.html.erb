<h1>Task</h1>

<h2>tasks</h2>
<% @tasks.each do |task| %>
<div>
  <%= task.title %>
</div>
  <%= task.start_time.strftime("%Y-%m-%d %H:%M") %>
  <%= link_to '編集', edit_task_path(task) %>
  <%= link_to '削除', task_path(task), method: :delete %>
  
<% end %>


  
  
  
  
  

